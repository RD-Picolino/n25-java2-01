package br.com.senaisp.bauru.secao02.aula02;

public class ArtAscii01 {

	public static void main(String[] args) {
		System.out.println("  OOOO  ");
		System.out.println( "O     O");
		System.out.println("O O  O O");
		System.out.println("O      O");
		System.out.println("O O  O O");
		System.out.println("O  OO  O");
		System.out.println(" O    O ");
		System.out.println("  OOOO  ");
	}

}
